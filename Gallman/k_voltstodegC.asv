
in = kg(103);

%define input variables
Rig = in.info.folder(end);
temp = [in.info.temptims];

%define variables for calculations
    %constants
    c1 = 1.009249522e-03;
    c2 = 2.378405444e-04; 
    c3 = 2.019202697e-07;

%thermister calibration by tank
    R1a = 9630; % adjust for each from 10000 //9500
    R1b = 9800; %adjust for each from 10000 //9500//11500
    R1c = 11000; % adjust for each from 10000 //9500


    if Rig == 'A'
        R1 = R1a;
    end
    
    if Rig == 'B'
        R1 = R1b;
    end
    
    if Rig == 'C'
        R1 = R1c;
    end
    


for j = 1:length(temp)
  
  R2 = R1 * ((1023.0 / temp(j)) - 1.0);
  logR2 = log(R2);
  T = (1.0 / (c1 + c2*logR2 + c3*logR2*logR2*logR2));
  tempC(j,:) = T - 273.15;

 
















